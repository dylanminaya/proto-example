{
  "name": "auth-proxy",
  "version": "1.0.0",
  "description": "gRPC to REST proxy for auth service",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "proto:generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --proto_path=../backend_test_one/proto ../backend_test_one/proto/auth.proto --ts_proto_opt=esModuleInterop=true,useOptionals=messages,outputJsonMethods=false,outputClientImpl=false"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "protobufjs": "^7.2.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "ts-proto": "^1.165.0"
  }
}
